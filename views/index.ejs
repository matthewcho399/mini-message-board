<html>
  <body>
    <h1><%= title%></h1>
    <a href="/new">Create Message</a>
    <div>
      <% messages.map((message, index) => { %>
      <h3><%= message.user%></h3>
      <p><%= message.text%></p>
      <p><%= message.added%></p>
      <form action="/message/<%= index %>">
        <input type="submit" value="Open" />
      </form>
      <% }) %>
    </div>
  </body>
</html>
