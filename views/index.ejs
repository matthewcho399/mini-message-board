<html>
  <body>
    <h1><%= title%></h1>
    <a href="/new">Create Message</a>
    <div>
      <% messages.map(message => { %>
      <h3><%= message.name%></h3>
      <p><%= message.text%></p>
      <p><%= message.added.toLocaleString()%></p>
      <form action="/message/<%= message.id %>">
        <input type="submit" value="Open" />
      </form>
      <% }) %>
    </div>
  </body>
</html>
